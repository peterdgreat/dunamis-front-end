    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "^6.1.1",
    "react-scripts": "4.0.3",
    "redux-logger": "^3.0.6"
